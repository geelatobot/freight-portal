    "test:unit": "jest --config jest.config.js --testPathPattern='\\.service\\.spec\\.ts$|\\.controller\\.spec\\.ts$'",
    "test:service": "jest --config jest.config.js --testPathPattern='\\.service\\.spec\\.ts$'",
    "test:controller": "jest --config jest.config.js --testPathPattern='\\.controller\\.spec\\.ts$'",
